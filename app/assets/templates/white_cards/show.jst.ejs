<div class="card-body">
  <%= white_card.get("body") %>
</div>

<div class="vote-area row">
  <div class="card-vote upvote">
    <span class="glyphicon glyphicon-thumbs-up"></span>
  </div>
  
  <div class="card-votes">
    <%= white_card.get("votes") %>
  </div>
  
  <div class="card-vote downvote">
    <span class="glyphicon glyphicon-thumbs-up"></span>
  </div>
</div>
