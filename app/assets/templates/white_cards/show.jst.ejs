<div class="card-body">
  <%= white_card.get("body") %>
</div>

<div class="vote-area">
  <div class="card-vote upvote">
    <span class="glyphicon glyphicon-thumbs-up"></span>
  </div>
  
  <div class="card-votes">
    <%= white_card.get("votes") %>
  </div>
  
  <div class="card-vote downvote">
    <span class="glyphicon glyphicon-thumbs-up"></span>
  </div>
</div>

<div class="new-white-card">
  <button class="btn" id="new-white-card-button">
    Submit
  </button>
</div>